{
  "check_interval": 900,
  "logging": {
    "level": "INFO",
    "console": false,
    "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s",
    "date_format": "%Y-%m-%d %H:%M:%S",
    "rotation": {
      "max_bytes": 5242880,
      "backup_count": 5
    },
    "module_levels": {
      "urllib3": "WARNING",
      "requests": "WARNING",
      "urllib3.connectionpool": "ERROR"
    }
  }
}